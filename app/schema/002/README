-- Find all linkages originating from 'UF'
(olass@localhost) [olass]>
SELECT
    linkage_id, partner_code, rule_code, lower(HEX(linkage_uuid)), lower(HEX(linkage_hash))
FROM
    linkage
    JOIN partner USING (partner_id)
    JOIN rule USING (rule_id)
WHERE
    partner_code = 'UF'
;

+------------+--------------+-----------+----------------------------------+------------------------------------------------------------------+
| linkage_id | partner_code | rule_code | lower(HEX(linkage_uuid))         | lower(HEX(linkage_hash))                                         |
+------------+--------------+-----------+----------------------------------+------------------------------------------------------------------+
|          1 | UF           | F_L_D_Z   | 006081360bc211e6af38933b6d71ed52 | 8b31efa965d46f971426ac9c133db1c769a712657b74410016d636b10a996506 |
|          2 | UF           | L_F_D_Z   | 006090f40bc211e6af38933b6d71ed52 | db07840bf253e5e6c16cabaca97fcc4363643f8552d65ec04290f3736d72b27d |
+------------+--------------+-----------+----------------------------------+------------------------------------------------------------------+

-- Find the 'OneFLorida ID' by providing a hash of the patient data
(olass@localhost) [olass]>
SELECT
    linkage_id, partner_code, rule_code, lower(HEX(linkage_uuid)), lower(HEX(linkage_hash))
FROM
    linkage
    JOIN partner USING (partner_id)
    JOIN rule USING (rule_id)
WHERE
    linkage_hash = lower(UNHEX(SHA2('first-last-dob-zip', 256)));

+------------+--------------+-----------+----------------------------------+------------------------------------------------------------------+
| linkage_id | partner_code | rule_code | lower(HEX(linkage_uuid))         | lower(HEX(linkage_hash))                                         |
+------------+--------------+-----------+----------------------------------+------------------------------------------------------------------+
|          1 | UF           | F_L_D_Z   | 006081360bc211e6af38933b6d71ed52 | 8b31efa965d46f971426ac9c133db1c769a712657b74410016d636b10a996506 |
+------------+--------------+-----------+----------------------------------+------------------------------------------------------------------+
